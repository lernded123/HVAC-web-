---
import BaseLayout from "src/layouts/BaseLayout.astro";
// import CTA from "@components/CTA.astro";
import extra from "../assets/images/hvac/extra.jpg"
---
<BaseLayout
title="Projects2"
description="Meta description for the page"
preloadImg={import(extra.src)}
>
<!-- ============================================ -->
  <!--                    LANDING                   -->
  <!-- ============================================ -->

  <section id="int-hero">
    <h1 id="home-h">Our Projects</h1>
    <picture>
      <source
        media="(max-width: 600px)"
        srcset={extra.src}
      />
      <source
        media="(min-width: 601px)"
        srcset={extra.src}
      />
      <img
        aria-hidden="true"
        decoding="async"
        srcset={extra.src}
        alt="kitchen cabinets"
        loading="eager"
        width="2500"
        height="1667"
      />
    </picture>
  </section>

 <!-- ============================================ -->
  <!--                   Gallery                    -->
  <!-- ============================================ -->





</BaseLayout>

